                                                                                                                       // 12568
                var stepTitles = [];                                                                                   // 12569
                if (this.wizardConfigs.stepTitles) {                                                                   // 12570
                    stepTitles = this.wizardConfigs.stepTitles;                                                        // 12571
                } else {                                                                                               // 12572
                    // Prepare step titles                                                                             // 12573
                    steps.each(function(i) {                                                                           // 12574
                        var stepTitle = {                                                                              // 12575
                            "title": "",                                                                               // 12576
                            "description": ""                                                                          // 12577
                        };                                                                                             // 12578
