                }                                                                                                      // 7073
            }                                                                                                          // 7074
        },                                                                                                             // 7075
                                                                                                                       // 7076
        /**                                                                                                            // 7077
         * Renders DOM elements for this field.                                                                        // 7078
         *                                                                                                             // 7079
         * @param onSuccess {Function} onSuccess callback.                                                             // 7080
         */                                                                                                            // 7081
        renderField: function(onSuccess) {                                                                             // 7082
        },                                                                                                             // 7083
